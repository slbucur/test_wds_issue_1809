{
    "presets": [
        ["env", {
          "targets": {
            "browsers": [
              "Chrome >= 52",
              "FireFox >= 44",
              "Safari >= 7",
              "Explorer 11",
              "last 4 Edge versions"
            ]
          },
        "useBuiltIns": true
        }],
        "react", "stage-1", "stage-2"
    ],
    "plugins": [
        "react-hot-loader/babel"
    ],
    "env": {
        "development": {
            "plugins": [
                "react-hot-loader/babel"
            ]
        },
        "test": {
            "presets": [
                "env", "react", "stage-1", "stage-2"
            ]
        }
    }
}
